AnonIPLimit = 65536
AuthOnlyViaModule = false
ConfigWriteDelay = 0
ConnectDelay = 1
HideVersion = true
LoadModule = partyline
LoadModule = webadmin
LoadModule = adminlog
LoadModule = chansaver
LoadModule = log
MaxBufferSize = 1000
ProtectWebSessions = false
SSLCertFile = /home/erratic/.znc/znc.pem
SSLDHParamFile = /home/erratic/.znc/znc.pem
SSLKeyFile = /home/erratic/.znc/znc.pem
ServerThrottle = 0
Skin = _default_
StatusPrefix = *
Version = 1.7.3

<Listener listener0>
	AllowIRC = true
	AllowWeb = true
	IPv4 = true
	IPv6 = true
	Port = 6667
	SSL = false
	URIPrefix = /
</Listener>

